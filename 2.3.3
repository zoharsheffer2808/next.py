class Dog():
    count_animals = 0
    def __init__(self, name="Octavio", age=0):
        Dog.count_animals += 1 
        self._name = name
        self._age = age 

    def birthday(self):
        self._age += 1

    def get_age(self):
        return self._age

    def set_name(self, name):
        self._name = name

    def get_name(self):
        return self._name
        
def main():
    dog1 = Dog('Toy', 2)
    dog2 = Dog()
    print(dog1.get_name())
    print(dog2.get_name())
    dog1.set_name('stag')
    print(dog1.get_name())
    print(Dog.count_animals)

main()
    
